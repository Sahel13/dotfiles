endsnippet

snippet it "Italics"
\textit{$1}
endsnippet

snippet dot "Dot derivative notation"
\dot{$1}
endsnippet

snippet spl "Split environment"
\begin{split}
	$1
\end{split}
endsnippet

snippet sube "Sub-equations"
\begin{subequations}
	\begin{align}
		$0
	\end{align}
\end{subequations}
endsnippet

snippet ref "Reference"
\ref{$1}
endsnippet

snippet req "Cross reference an equation"
